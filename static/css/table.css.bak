div.table {
	display:table;
	border-collapse: collapse;
}
div.table-row{
	display:table-row;
}
div.table-row:hover{
	background-color: beige;
}
div.table-cell{
	border:dotted 1px gray;
	display:table-cell;
	padding-left: 5px;
}
div.table-header .table-cell{
	font-weight: bold;
	border:solid 1px black;
}
